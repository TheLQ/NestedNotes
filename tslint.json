{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "max-classes-per-file": false,
        "object-literal-sort-keys": false,
        "member-access": false,
        "no-console": false,
        "indent": [true, "tabs", 2],
        "interface-name": false
    },
    "rulesDirectory": []
}